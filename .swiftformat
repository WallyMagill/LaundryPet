--indent 4
--indentcase false
--trimwhitespace always
--maxwidth 100
--wraparguments before-first
--wrapparameters before-first
--wrapcollections before-first
--closingparen balanced
--commas inline
--decimalgrouping 3,4
--exponentgrouping 4
--fractiongrouping disabled
--hexgrouping 4,8
--octalgrouping 4,8
--binarygrouping 4,8
--ifdef no-indent
--patternlet inline
--self remove
--importgrouping testable-bottom
--stripunusedargs closure-only
--exclude .build,DerivedData,Pods,.vscode,.cursor

